  "scripts": {
    "up": "docker-compose -f docker-compose.dev.yml up -d",
    "stop": "docker-compose -f docker-compose.dev.yml stop",
    "down": "docker-compose -f docker-compose.dev.yml down && rm -rf data/*",
    "up:stage": "docker-compose -f docker-compose.stage.yml up --build -d",
    "stop:stage": "docker-compose -f docker-compose.stage.yml stop",
    "down:stage": "docker-compose -f docker-compose.stage.yml down && rm -rf data/*",
    "postup": "yarn dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "graphql-codegen --config codegen.yml",
    "cy:open": "cypress open",
    "cy:start": "cypress run",
    "cy:run-file": "cypress run --spec cypress/e2e/cms/attributes-groups.spec.ts",
