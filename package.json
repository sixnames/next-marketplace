  "scripts": {
    "up": "docker-compose -f docker-compose.dev.yml up -d",
    "stop": "docker-compose -f docker-compose.dev.yml stop",
    "down": "docker-compose -f docker-compose.dev.yml down && rm -rf data/*",
    "dev": "yarn up && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "graphql-codegen --config codegen.yml",
    "cy:open": "cypress open",
    "cy:run-file": "cypress run --spec cypress/e2e/cms/product-attributes.spec.ts",
    "test": "cypress run",
    "ci": "server-test 3000",
    "build-and-test": "yarn build && yarn ci",
    "generate-links": "node -r esbuild-register lib/links/generateProjectLinks.ts && prettier --write lib/links/getProjectLinks.ts",
