{
  "compilerOptions": {
    "target": "es5",
    "types": ["cypress", "node", "@4tw/cypress-drag-drop"],
    "baseUrl": "../",
    "moduleResolution": "node",
    "strict": true,
    "lib": ["es5", "es2016", "dom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "isolatedModules": false,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["../node_modules"]
}
