{
  "compilerOptions": {
    "baseUrl": "../",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "jsx": "react",
    "lib": ["es5", "es2016", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es5",
    "types": ["cypress"],
  },
  "include": ["./**/*.ts"],
  "exclude": ["../node_modules"]
}
