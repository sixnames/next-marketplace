{
  "compilerOptions": {
    "target": "es5",
    "types": ["cypress", "node"],
    "baseUrl": "../",
    "moduleResolution": "node",
    "strict": true,
    "lib": ["es5", "es2016", "dom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "isolatedModules": false,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "../node_modules"
  ]
}
