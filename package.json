{
  "name": "winepoint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "up": "docker-compose -f docker-compose.dev.yml up -d",
    "stop": "docker-compose -f docker-compose.dev.yml stop",
    "down": "docker-compose -f docker-compose.dev.yml down && rm -rf data/*",
    "up:stage": "docker-compose -f docker-compose.stage.yml up --build -d",
    "stop:stage": "docker-compose -f docker-compose.stage.yml stop",
    "down:stage": "docker-compose -f docker-compose.stage.yml down && rm -rf data/*",
    "postup": "yarn dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate": "graphql-codegen --config codegen.yml",
    "cy:open": "cypress open",
    "cy:start": "cypress run",
    "cy:run-file": "cypress run --spec cypress/e2e/cms/attributes-groups.spec.ts",
    "test": "npm run cy:start",
    "create-indexes": "npx ts-node -O '{\"module\":\"commonjs\"}' db/initialData/indexes.ts",
    "seed-test-data": "env-cmd seed --drop-database ./db/initialData/initial && env-cmd seed ./db/initialData/parsed && env-cmd seed ./db/initialData/parsedBigCollections/allProducts && env-cmd seed ./db/initialData/parsedBigCollections/wine-a && env-cmd seed ./db/initialData/parsedBigCollections/wine-b && env-cmd seed ./db/initialData/parsedBigCollections/wine-c && env-cmd seed ./db/initialData/parsedBigCollections/wine-d && yarn create-indexes"
  },
